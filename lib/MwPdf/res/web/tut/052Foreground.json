{
  "meta": {
    "themes": {
      "@def": {
        "format": {
          "preset": "a5"
        },
        "background": {
          "FullPage": {
            "ignoreMargins": true,
            "child": {
              "Container": {
                "color": "BBBBBB"
              }
            }
          }
        },
        "foreground": {
          "FullPage": {
            "ignoreMargins": true,
            "child": {
              "Center": {
                "child": {
                  "Stack": {
                    "overflow": "visible",
                    "children": [
                      {
                        "Transform": {
                          "alignment": "center",
                          "transform": {
                            "RotateZ": {
                              "angle": 30
                            }
                          },
                          "child": {
                            "Opacity": {
                              "opacity": 0.7,
                              "child": {
                                "Widget": {
                                  "source": "@classified"
                                }
                              }
                            }
                          }
                        }
                      },
                      {
                        "Transform": {
                          "alignment": "center",
                          "transform": {
                            "RotateZ": {
                              "angle": 40
                            }
                          },
                          "child": {
                            "Opacity": {
                              "opacity": 0.2,
                              "child": {
                                "Widget": {
                                  "source": "@classified"
                                }
                              }
                            }
                          }
                        }
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "pages": [
    {
      "theme": "@def",
      "content": [
        {
          "LoremParagraph": {
            "sentence": 200,
            "style": {
              "color": "FFFFFF"
            }
          }
        }
      ]
    }
  ],
  "sources": {
    "@classified": {
      "widget": {
        "Container": {
          "decoration": {
            "borderRadius": [
              1
            ],
            "border": {
              "all": {
                "color": "990000",
                "width": 2
              }
            }
          },
          "child": {
            "Padding": {
              "padding": [
                5,
                3
              ],
              "child": {
                "Text": {
                  "style": {
                    "fontWeight": "bold",
                    "color": "990000",
                    "fontSize": 10
                  },
                  "text": "CLASSIFIED"
                }
              }
            }
          }
        }
      }
    }
  }
}